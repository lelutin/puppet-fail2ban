{
  "name": "LeLutin-fail2ban",
  "version": "4.0.1",
  "author": "LeLutin",
  "summary": "Manage fail2ban bruteforce protector",
  "license": "GPL-3.0+",
  "source": "https://github.com/lelutin/puppet-fail2ban",
  "project_page": "https://github.com/lelutin/puppet-fail2ban",
